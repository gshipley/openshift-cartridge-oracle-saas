#!/bin/bash

source $OPENSHIFT_CARTRIDGE_SDK_BASH

OPENSHIFT_ORACLE_PIDFILE=${OPENSHIFT_ORACLE__DIR}run/oracle.pid
OPENSHIFT_ORACLESRC=${OPENSHIFT_DATA_DIR}.oraclesrc

function start() {
 :
}

function stop() {
  :
}

function restart() {
    stop
    start
}

function status() {
  return 0
}

function reload() {
  :
}

function tidy() {
  echo "Nothing to clean for oracle."
}

case "$1" in
  start) start ;;
  stop) stop ;;
  restart) restart ;;
  status) status ;;
  reload) reload ;;
  tidy) tidy ;;
  *) exit 0
esac

